{
  "extends": "../../tsconfig.base.json",
  "include": [
    "./*",
  ],
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationMap": false,
    "sourceMap": true,
    "module": "es2015",
    "target": "es6",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es6"
    ],
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "references": [
    {
      "path": "../mezon-js-protobuf"
    }
  ]
}