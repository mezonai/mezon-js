{
  "compilerOptions": {
    "lib": [
      "ES2022", "DOM"
    ],
    "target": "ES2020", 
    "module": "ESNext",
    "removeComments": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "noEmitHelpers": false,
    "importHelpers": false,
    "baseUrl": "./",
    "noEmit": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "./*",
  ],
  "exclude": []
}
